{
    "name": "@codinators/gatsby-shared-library",
    "version": "1.1.2",
    "description": "Shared components for gatsby blogs",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "repository": "https://github.com/AnkurSheel/gatsby_shared_library.git",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "license": "MIT",
    "private": false,
    "files": [
        "lib"
    ],
    "types": "./lib/types/index.d.ts",
    "scripts": {
        "prebuild": "rimraf ./lib/*",
        "build": "cross-env-shell npm-run-all --parallel types compile",
        "types": "tsc",
        "compile": "cross-env BABEL_ENV=esmodules babel --root-mode upward src --out-dir lib --extensions \".ts,.tsx\"",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "yarn lint --fix",
        "storybook": "cross-env BABEL_ENV=storybook start-storybook -s public",
        "build-storybook": "cross-env BABEL_ENV=storybook build-storybook -s public",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz"
    },
    "dependencies": {
        "@emotion/core": "^10.0.17",
        "@storybook/addon-knobs": "^5.2.0",
        "@storybook/react": "^5.2.0",
        "react": "^16.9.0",
        "react-dom": "^16.9.0",
        "ts-node": "^8.3.0",
        "ts-optchain": "^0.1.8"
    },
    "devDependencies": {
        "@babel/cli": "^7.6.2",
        "@babel/core": "^7.6.2",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "@babel/preset-react": "^7.0.0",
        "@babel/preset-typescript": "^7.6.0",
        "@emotion/babel-preset-css-prop": "^10.0.17",
        "@semantic-release/changelog": "^3.0.4",
        "@semantic-release/git": "^7.0.16",
        "@storybook/addon-actions": "^5.2.0",
        "@storybook/addon-info": "^5.2.0",
        "@storybook/addon-links": "^5.2.0",
        "@storybook/addons": "^5.2.0",
        "@storybook/cli": "^5.2.0",
        "@types/prettier": "^1.18.2",
        "@types/react": "^16.9.2",
        "@types/storybook__addon-knobs": "^5.0.3",
        "@types/storybook__react": "^4.0.2",
        "@typescript-eslint/eslint-plugin": "^2.2.0",
        "@typescript-eslint/parser": "^2.2.0",
        "babel-loader": "^8.0.6",
        "commitizen": "^4.0.3",
        "cross-env": "^6.0.0",
        "cz-conventional-changelog": "^3.0.2",
        "eslint": "^6.4.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.3.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.14.3",
        "eslint-plugin-react-hooks": "^2.0.1",
        "husky": "^3.0.5",
        "lint-staged": "^9.2.5",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.18.2",
        "react-docgen-typescript-loader": "^3.2.0",
        "rimraf": "^3.0.0",
        "semantic-release": "^15.13.24",
        "typescript": "^3.6.3",
        "yalc": "^1.0.0-pre.34"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
