{
    "name": "@codinators/gatsby-shared-library",
    "version": "1.6.0",
    "description": "Shared components for gatsby blogs",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "repository": "https://github.com/AnkurSheel/gatsby_shared_library.git",
    "author": "Ankur Sheel <ankur.sheel@gmail.com>",
    "license": "MIT",
    "private": false,
    "files": [
        "lib"
    ],
    "types": "./lib/types/index.d.ts",
    "scripts": {
        "prebuild": "rimraf ./lib/*",
        "build": "cross-env-shell npm-run-all --parallel types compile",
        "types": "tsc",
        "compile": "cross-env BABEL_ENV=esmodules babel src --out-dir lib --extensions \".ts,.tsx\"",
        "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
        "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "yarn lint --fix",
        "storybook": "cross-env BABEL_ENV=storybook start-storybook -s public",
        "build-storybook": "cross-env BABEL_ENV=storybook build-storybook -s public",
        "semantic-release": "semantic-release",
        "commit": "yarn git-cz",
        "deduplicate": "yarn-deduplicate --list && yarn-deduplicate",
        "test": "cross-env BABEL_ENV=test jest",
        "test:watch": "jest --watch"
    },
    "dependencies": {
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-jss": "^10.1.1",
        "ts-optchain": "^0.1.8"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-react": "^7.9.4",
        "@babel/preset-typescript": "^7.9.0",
        "@codinators/eslint-config": "^1.0.0",
        "@commitlint/cli": "^9.1.2",
        "@commitlint/config-conventional": "^9.1.2",
        "@semantic-release/changelog": "^5.0.1",
        "@semantic-release/git": "^9.0.0",
        "@storybook/addon-actions": "^6.0.21",
        "@storybook/addon-info": "^5.3.18",
        "@storybook/addon-knobs": "^6.0.21",
        "@storybook/addon-links": "^6.0.21",
        "@storybook/addons": "^6.0.21",
        "@storybook/cli": "^6.0.21",
        "@storybook/react": "^6.0.21",
        "@testing-library/jest-dom": "^5.3.0",
        "@testing-library/react": "^11.0.4",
        "@types/jest": "^26.0.12",
        "@types/jest-axe": "^3.2.1",
        "@types/prettier": "^2.1.0",
        "@types/react": "^16.9.31",
        "@types/react-helmet": "^6.1.0",
        "@types/storybook__react": "^5.2.1",
        "@typescript-eslint/eslint-plugin": "^4.0.1",
        "@typescript-eslint/parser": "^4.0.1",
        "babel-loader": "^8.1.0",
        "commitizen": "^4.0.3",
        "cross-env": "^7.0.2",
        "cz-conventional-changelog": "^3.1.0",
        "eslint": "^7.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^4.1.0",
        "husky": "^4.2.3",
        "jest": "^26.4.2",
        "jest-axe": "^3.4.0",
        "jest-junit": "^11.1.0",
        "lint-staged": "^10.1.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.2",
        "react-docgen-typescript-loader": "^3.7.2",
        "rimraf": "^3.0.2",
        "semantic-release": "^17.0.4",
        "ts-jest": "^26.3.0",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2",
        "webpack": "^4.42.1",
        "yalc": "^1.0.0-pre.35",
        "yarn-deduplicate": "^3.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    }
}
